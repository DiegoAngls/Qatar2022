values_to = "Bet") %>%
nest(-`Match`)
result_pics <- lapply(1:16,function(x){
freqs <- bets2$data[[x]]$Bet %>%
table()
prop <- prop.table(freqs)
return(prop)
}) %>%
setNames(bets2$Match)
png("media/01.picks_stage1.png",width = 20,height = 20,units = "cm",
res = 196)
par(mfrow = c(4,4),
mar = c( 3,3,3,3),
